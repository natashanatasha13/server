{
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "jsx": "react",
    "target": "es6",
    "types": ["node", "@types/node"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules"],
  "noImplicitAny": true,
  "strictNullChecks": true
}
